{
  "name": "react-native-appstore",
  "version": "0.0.1",
  "engines" : {
    "node" : ">=7.6"
  },
  "private": true,
  "scripts": {
    "postinstall": "sh ./scripts/postinstall.sh",
    "reload-env": "sh ./scripts/dotenv.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint src",
    "seed": "babel-node ./scripts/seed.js"
  },
  "eslintIgnore": [
    "config.env.js"
  ],
  "dependencies": {
    "algoliasearch": "3.24.6",
    "apollo-cache-inmemory": "1.1.1",
    "apollo-client": "2.0.3",
    "apollo-link-http": "1.2.0",
    "core-decorators": "0.20.0",
    "date-fns": "1.29.0",
    "graphql": "0.11.7",
    "graphql-tag": "2.5.0",
    "lodash": "4.17.4",
    "mobx": "3.3.1",
    "mobx-persist": "0.3.4",
    "mobx-react": "4.3.4",
    "prop-types": "15.6.0",
    "react": "16.1.0",
    "react-apollo": "2.0.1",
    "react-native": "0.50.3",
    "react-native-blur": "3.2.2",
    "react-native-code-push": "5.1.3-beta",
    "react-native-config": "0.6.0",
    "react-native-firebase": "3.1.1",
    "react-native-navigation": "1.1.296",
    "react-native-sentry": "0.30.3",
    "react-native-snap-carousel": "3.4.0",
    "recompose": "0.26.0"
  },
  "devDependencies": {
    "@playlyfe/gql": "2.3.2",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "detox": "6.0.2",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-babel-module": "4.0.0-beta.3",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.4.2",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "3.1.0",
    "faker": "4.1.0",
    "jest": "21.2.1",
    "node-fetch": "1.7.3",
    "react-dom": "16.2.0",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "16.1.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./scripts/jest.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/AppStoreClone.app",
        "build": "xcodebuild -workspace ios/AppStoreClone.xcworkspace -scheme AppStoreClone -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/AppStoreClone.app",
        "build": "xcodebuild -workspace ios/AppStoreClone.xcworkspace -scheme AppStoreClone -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      }
    }
  }
}
